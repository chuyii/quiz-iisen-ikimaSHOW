{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    ".read": true,
    ".write": true,
    "questions": {
      ".indexOn": ["id"]
    },
    "answers": {
      ".indexOn": ["userId", "questionId"]
    },
    "ratings": {
      ".indexOn": ["userId"]
    }
  }
}
